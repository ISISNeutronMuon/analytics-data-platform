connector.name=iceberg
iceberg.catalog.type=rest
iceberg.rest-catalog.security=NONE
iceberg.rest-catalog.vended-credentials-enabled=true
iceberg.rest-catalog.uri={{ nessie_iceberg_catalog.uri }}
iceberg.rest-catalog.prefix=main

# Vended credentials should mean this configuration is not necessary but
# without it Trino can't access the object store
fs.native-s3.enabled=true
s3.endpoint={{ s3_endpoint }}
s3.region={{ s3_region }}
s3.path-style-access=true
s3.aws-access-key={{ s3_access_key_id }}
s3.aws-secret-key={{ s3_access_secret }}
