http:
  routers:
    # TLS
    superset:
      entryPoints: web_tls
      rule: Host(`{{ top_level_domain }}`) && PathPrefix(`{{ superset_app_root }}`)
      service: superset
      tls: {}

  services:
    superset:
      loadBalancer:
        servers:
          - url: "http://superset:{{ superset_http_port }}"
